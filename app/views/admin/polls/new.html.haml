.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to t("web-app-theme.list", :default => "List"), admin_polls_path
      %li.active= link_to t("web-app-theme.new", :default => "New"), new_admin_poll_path
  .content
    %h2.title #{ t("web-app-theme.new", :default => "New")} Poll
    .inner
      - form_for @poll, :url => admin_polls_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')